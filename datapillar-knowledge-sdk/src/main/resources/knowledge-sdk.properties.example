# Knowledge SDK 配置示例
# 复制此文件为 knowledge-sdk.properties 并修改配置

# Neo4j 连接配置
knowledge.neo4j.uri=bolt://localhost:7687
knowledge.neo4j.username=neo4j
knowledge.neo4j.password=your_password
knowledge.neo4j.database=neo4j
knowledge.neo4j.max-pool-size=50

# SDK 配置
knowledge.producer=your_app_name
knowledge.async=true
knowledge.batch-size=100
knowledge.flush-interval=5s

# 重试配置
knowledge.retry.max-retries=3
knowledge.retry.initial-delay=100ms
knowledge.retry.max-delay=5s
